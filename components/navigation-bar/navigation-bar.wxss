.nav-bar{ 
  z-index: 1;
  position: fixed; width: 100%; top: 0; color: #fff; background: #ff0000;
}
.nav-bar .search{ 
  width: 50%; 
  color: #333; 
  font-size: 14px; 
  background: #fff; 
  position: absolute; 
  border-radius: 50px; 
  background: rgba(255, 255, 255, 0.945); 
  padding-left: 33px;
}
.nav-bar image{
  z-index: 2;
  width: 35rpx;
  /* height: 40rpx; */
  position: absolute; 
}
